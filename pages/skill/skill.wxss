/* pages/skill/skill.wxss */
.searchInput{
  border: 1px solid #e4e4e4 ;
  width: 90%;
  margin: 30rpx auto;
  padding: 14rpx;
  border-radius: 20rpx;
}

/* 列表样式 */
.serviceList{
  overflow: auto;
}
.swiper_con{
  padding:20rpx;
  border-bottom: 1px solid #e4e4e4;
}
.dataImg{
  border-radius: 4%;
  width: 200rpx;
  height: 300rpx;
}
.dataInfo{
  /* border: 1px solid red; */
  width: calc(100% - 230rpx);
  display: inline-block;
  vertical-align: top;
  padding: 0 10rpx;
}
.dataInfo>text{
  display: block
}
.dataInfo>.title{
  font-size: 65rpx;
  line-height: 1.5;
}
.dataInfo>.number{
  line-height: 1.5;
  font-size: 40rpx;
}
.dataInfo>.desc{
  width: 80%;
  display: inline-block;
  white-space:normal;
  word-break:normal;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
    font-size: 26rpx;
    color: #666;
}
.score{
  font-size: 40rpx;
  text-align: center;
  float: right;
  margin-top: 20%;
  color: #d24a58;
}
 