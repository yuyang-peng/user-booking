<!--pages/skill/skill.wxml-->
<!-- 搜索框 -->
<input confirm-type="search" value="" placeholder="请输入想要搜索的理发师" class="searchInput"></input>

<!-- 附近服务 -->
<view class="serviceList">
  <view wx:for="{{serviceList}}" wx:key="id" class="swiper_con" data-skillId="{{item.id}}" bindtap="toSkill">
      <image class="dataImg" webp="True" src="http://localhost:8080{{item.image}}"></image>
      <view class="dataInfo">
        <text class="score">评分\n{{item.score}}</text>
        <text class="name">姓名:{{item.barberName}}</text>
        <text class="age">年龄:{{item.barberAge}}</text>
        <text class="phone">phone:{{item.barberPhone}}</text>
      </view>
    </view>
</view>